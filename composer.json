{
    "name": "lappuse/composer-installer",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Mert Kabadayi",
            "email": "mert.kabadayi@smartpixel.com.tr"
        }
    ],
    "require": {
        "composer-plugin-api": "1.0.0"
    },
    "require-dev": {
        "composer/composer": "1.0.*@dev",
        "phpunit/phpunit": "4.1.*",
		"phpunit/phpunit-mock-objects": "dev-master",
		"squizlabs/php_codesniffer": "1.4.7"
    },
    "autoload": {
        "psr-0": {
            "Lappuse\\Composer\\Installer\\": "src/"
        }
    },
    "extra": {
        "class": "Lappuse\\Composer\\Installer\\Plugin",
		"branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
